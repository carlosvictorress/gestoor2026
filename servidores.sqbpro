ALTER TABLE caee_plano_atendimento RENAME TO caee_plano_atendimento_old;

-- 2. Criar a tabela NOVA (Exatamente igual, mas SEM a linha 'UNIQUE')
CREATE TABLE &quot;caee_plano_atendimento&quot; (
    &quot;id&quot; INTEGER NOT NULL,
    &quot;aluno_id&quot; INTEGER NOT NULL,
    &quot;profissional_id&quot; INTEGER NOT NULL,
    &quot;status_plano&quot; VARCHAR(50) NOT NULL,
    &quot;data_inicio&quot; DATE NOT NULL,
    &quot;frequencia_semanal&quot; INTEGER,
    &quot;duracao_sessao_min&quot; INTEGER,
    &quot;objetivos_gerais&quot; TEXT,
    &quot;objetivos_especificos&quot; TEXT,
    &quot;metodologia&quot; TEXT,
    PRIMARY KEY(&quot;id&quot;),
    FOREIGN KEY(&quot;aluno_id&quot;) REFERENCES &quot;caee_aluno&quot;(&quot;id&quot;),
    FOREIGN KEY(&quot;profissional_id&quot;) REFERENCES &quot;caee_profissional&quot;(&quot;id&quot;)
);

-- 3. Copiar os dados da tabela velha para a nova
INSERT INTO caee_plano_atendimento SELECT * FROM caee_plano_atendimento_old;

-- 4. Apagar a tabela velha
DROP TABLE caee_plano_atendimento_old;</sql><current_tab id="0"/></tab_sql></sqlb_project>
